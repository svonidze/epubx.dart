[access]
	inheritFrom = All-Projects
[submit]
[project]
	description = Epub Reader and Writer for Dart
[access "refs/*"]
	owner = group user/svonidze
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group io.smotion.shlang
	label-Code-Review = -2..+2 group user/svonidze
	label-Verified = -1..+1 group user/svonidze
	label-Verified = -1..+0 group io.smotion.shlang
	push = group io.smotion.shlang
	push = group user/svonidze
	read = group io.smotion.shlang
	read = group user/svonidze
	rebase = group io.smotion.shlang
	rebase = group user/svonidze
	removeReviewer = group io.smotion.shlang
	removeReviewer = group user/svonidze
	submit = group io.smotion.shlang
	submit = group user/svonidze
[access "refs/heads/*"]
	create = group user/svonidze
	push = +force group user/svonidze
	pushMerge = group user/svonidze
	read = group io.smotion.shlang
	read = group user/svonidze
[access "refs/tags/*"]
	push = group user/svonidze
	push = +force group user/svonidze
